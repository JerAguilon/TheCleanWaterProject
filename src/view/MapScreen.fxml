<?xml version="1.0" encoding="UTF-8"?>

<?import com.lynden.gmapsfx.GoogleMapView?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<AnchorPane id="AnchorPane" prefHeight="500" prefWidth="750" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MapController">
    <children>
        <GoogleMapView fx:id="mapView" prefHeight="500.0" prefWidth="700.0" AnchorPane.bottomAnchor="0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0.0" />
        <TextField fx:id="addressTextField" prefHeight="27.0" prefWidth="274.0" promptText="Address" AnchorPane.leftAnchor="10" AnchorPane.topAnchor="10" />
      <BorderPane prefHeight="500.0" prefWidth="103.0">
         <bottom>
              <Button fx:id="doneButton" mnemonicParsing="false" onAction="#returnToMain" prefHeight="39.0" prefWidth="91.0" text="Done" BorderPane.alignment="CENTER" />
         </bottom>
      </BorderPane>
    </children>


</AnchorPane>
